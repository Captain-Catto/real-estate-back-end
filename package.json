  "version": "1.0.0",
  "description": "A backend application built with TypeScript and Express.",
  "main": "src/app.ts",
  "scripts": {
    "start": "ts-node src/app.ts",
    "dev": "nodemon src/app.ts",
    "build": "tsc",
    "import-location": "ts-node scripts/importLocation.ts",
