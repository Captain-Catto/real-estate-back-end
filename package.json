    "multer-s3": "^2.10.0",
    "node-cron": "^4.2.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.13",
