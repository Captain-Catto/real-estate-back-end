    "test-update-package": "ts-node scripts/testUpdatePackage.ts",
    "test-notifications": "ts-node scripts/testNotificationSystem.ts",
    "test-notifications-simple": "ts-node scripts/testNotificationSimple.ts",
    "create-admin": "ts-node scripts/createAdminUser.ts",
    "seed:areas-prices": "ts-node scripts/seedAreasAndPrices.ts",
    "update-avatars": "ts-node src/scripts/updateUsersAvatar.ts",
